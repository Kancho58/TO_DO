{
  "name": "to_do",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "nodemon --exec ts-node src/index.ts",
    "seed": "knex seed:run --knexfile=knexfile.ts --verbose",
    "migrate": "knex migrate:latest --knexfile=knexfile.ts --verbose",
    "rollback": "knex migrate:rollback --knexfile=knexfile.ts --verbose",
    "make:seeder": "knex seed:make --knexfile=knexfile.ts -x ts",
    "make:migration": "knex migrate:make --knexfile=knexfile.ts -x ts",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "http-status-codes": "^2.2.0",
    "joi": "^17.9.2",
    "knex": "^2.4.2",
    "lodash": "^4.17.21",
    "pg": "^8.11.0",
    "winston": "^3.9.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/knex": "^0.16.1",
    "@types/lodash": "^4.14.195",
    "@types/node": "^20.3.1",
    "@types/pg": "^8.10.2",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "eslint": "^8.43.0",
    "nodemon": "^2.0.22",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3"
  }
}
